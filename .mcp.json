{
  "mcpServers": {
    "context7": {
      "type": "http",
      "url": "https://mcp.context7.com/mcp"
    },
    "nuxt": {
      "type": "sse",
      "url": "http://localhost:4000/__mcp/sse"
    },
    "prisma": {
      "type": "http",
      "url": "https://mcp.prisma.io/mcp"
    },
    "playwright": {
      "command": "pnpx",
      "args": [
        "@playwright/mcp@latest"
      ]
    },
    "sequential-thinking": {
      "command": "pnpx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    },
    "nuxt-docs": {
      "type": "sse",
      "url": "https://mcp.nuxt.com/sse"
    }
  }
}
