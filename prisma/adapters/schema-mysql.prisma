  twitter_image                  String?   @db.MediumText
  twitter_description            String?   @db.MediumText
  twitter_image_id               String?
  twitter_image_source           String?   @db.Text
  open_graph_title               String?   @db.Text
  open_graph_description         String?   @db.MediumText
  open_graph_image               String?   @db.MediumText
  open_graph_image_id            String?
  open_graph_image_source        String?   @db.Text
  open_graph_image_meta          String?   @db.Text
  link_count                     Int?
  incoming_link_count            Int?
  prominent_words_version        Int?      @db.UnsignedInt
  created_at                     DateTime? @db.DateTime(0)
  updated_at                     DateTime  @default(now()) @db.Timestamp(0)
