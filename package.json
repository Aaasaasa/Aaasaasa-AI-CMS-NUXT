{
  "name": "aaasaasa-ai-web-client",
  "private": true,
  "type": "module",
  "version": "0.0.1",
  "author": "Aleksandar Stajic",
  "license": "MIT",
  "homepage": "https://stajic.de",
  "repository": {
    "type": "git",
    "url": "https://github.com/Aaasaasa/Aaasaasa-AI-CMS-NUXT.git"
  },
  "bugs": {
    "url": "https://github.com/Aaasaasa/Aaasaasa-AI-CMS-NUXT/issues"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --port 4000 --host 0.0.0.0",
    "dev:docker": "node scripts/setup-data-isolation.js && nuxt dev --port 4000 --host 0.0.0.0",
    "dev:clean": "rm -rf .nuxt .output && node scripts/setup-data-isolation.js && nuxt dev --port 4000 --host 0.0.0.0",
    "dev:safe": "node scripts/setup-data-isolation.js && nuxt dev --port 4000 --host 0.0.0.0",
    "preview": "nuxt preview",
    "lint": "yarn run lint:prettier && yarn run lint:eslint",
    "lint:ci": "prettier --check . && yarn run lint:eslint",
    "lint:eslint": "eslint . --fix --ignore-pattern volumes/",
    "lint:prettier": "prettier --write --list-different .",
    "typecheck": "nuxt typecheck",
    "prepare": "husky",
    "test": "yarn run test:unit && yarn run test:e2e",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest --watch",
    "test:unit:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "prisma:generate": "yarn prisma:generate:postgres && yarn prisma:generate:mysql && yarn prisma:generate:mongo",
    "prisma:generate:postgres": "prisma generate --schema=prisma/adapters/schema-postgres.prisma",
    "prisma:generate:mysql": "prisma generate --schema=prisma/adapters/schema-mysql.prisma",
    "prisma:generate:mongo": "prisma generate --schema=prisma/adapters/schema-mongo.prisma",
    "prisma:migrate": "yarn prisma:migrate:postgres",
    "prisma:migrate:postgres": "prisma migrate dev --schema=prisma/adapters/schema-postgres.prisma",
    "prisma:migrate:mysql": "prisma migrate dev --schema=prisma/adapters/schema-mysql.prisma",
    "prisma:studio": "prisma studio --schema=prisma/adapters/schema-postgres.prisma",
    "prisma:studio:postgres": "prisma studio --schema=prisma/adapters/schema-postgres.prisma --port 5555",
    "prisma:studio:mysql": "prisma studio --schema=prisma/adapters/schema-mysql.prisma --port 5556",
    "prisma:reset": "yarn prisma:reset:postgres && yarn prisma:reset:mysql",
    "prisma:reset:postgres": "prisma migrate reset --schema=prisma/adapters/schema-postgres.prisma --force",
    "prisma:reset:mysql": "prisma migrate reset --schema=prisma/adapters/schema-mysql.prisma --force",
    "prisma:push": "yarn prisma:push:postgres && yarn prisma:push:mysql && yarn prisma:push:mongo",
    "prisma:push:postgres": "prisma db push --schema=prisma/adapters/schema-postgres.prisma",
    "prisma:push:mysql": "prisma db push --schema=prisma/adapters/schema-mysql.prisma",
    "prisma:push:mongo": "prisma db push --schema=prisma/adapters/schema-mongo.prisma",
    "db:seed": "yarn db:seed:postgres && yarn db:seed:mysql && yarn db:seed:mongo",
    "db:seed:postgres": "ts-node prisma/seed-data/postgres-seed.ts",
    "db:seed:mysql": "ts-node prisma/seed-data/mysql-seed.ts",
    "db:seed:mongo": "ts-node prisma/seed-data/mongo-seed.ts",
    "wp:clean": "ts-node scripts/clean-wordpress-dump.ts",
    "wp:test": "ts-node scripts/test-migration.ts",
    "wp:migrate": "ts-node migrate/wordpress-to-postgres.ts",
    "wp:migrate:media": "ts-node migrate/wordpress-media-migration.ts",
    "wp:import": "yarn wp:clean && yarn wp:migrate && yarn wp:migrate:media",
    "env:check": "sh -c '[ -f .env ] || (echo \"âŒ Missing .env file\" && exit 1)'",
    "env:copy": "cp .env.example .env",
    "migrate:wp": "ts-node migrate/migrate.ts",
    "changelog": "yarn dlx git-cliff --output CHANGELOG.md",
    "release:patch": "yarn version patch && git push --follow-tags",
    "release:minor": "yarn version minor && git push --follow-tags",
    "release:major": "yarn version major && git push --follow-tags",
    "clean": "yarn clean:root && yarn clean:workspaces && yarn clean:cache && yarn clean:prisma",
    "clean:root": "rm -rf node_modules yarn.lock .turbo .nuxt .output dist",
    "clean:workspaces": "find app server shared -type d \\( -name node_modules -o -name .nuxt -o -name .output -o -name .turbo \\) -exec rm -rf {} +",
    "clean:cache": "yarn cache clean",
    "clean:prisma": "rm -rf node_modules/.prisma node_modules/.prisma-client prisma/generated",
    "reinstall": "yarn clean && yarn install --check-files && yarn postinstall",
    "postinstall": "nuxt prepare",
    "bootstrap": "yarn run reinstall",
    "setup:data": "node scripts/setup-data-isolation.js setup",
    "restore:data": "node scripts/setup-data-isolation.js restore",
    "images:optimize": "node scripts/optimize-wordpress-images.js",
    "images:install": "yarn add sharp glob --dev"
  },
  "dependencies": {
    "@iconify-json/lucide": "^1.2.70",
    "@iconify-json/openmoji": "^1.2.17",
    "@nuxt/image": "^1.11.0",
    "@nuxt/ui": "^4.0.1",
    "@nuxtjs/i18n": "^10.1.1",
    "@nuxtjs/seo": "^3.2.2",
    "@pinia/nuxt": "^0.11.2",
    "@prisma/client": "^7.0.0",
    "@types/pg": "^8.15.6",
    "@types/prismjs": "^1.26.5",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.3",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^7.0.9",
    "nuxt": "^4.1.3",
    "nuxt-auth-utils": "^0.5.25",
    "nuxt-nodemailer": "^1.1.2",
    "nuxt-security": "^2.4.0",
    "pg": "^8.16.3",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.5.0",
    "prismjs": "^1.30.0",
    "redis": "^5.9.0",
    "vue-i18n": "^11.1.12",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@nuxt/eslint": "^1.9.0",
    "@nuxt/test-utils": "^3.19.2",
    "@playwright/test": "^1.56.1",
    "@types/bcryptjs": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.9.1",
    "eslint": "^9.38.0",
    "glob": "^11.0.3",
    "happy-dom": "^20.0.8",
    "husky": "^9.1.7",
    "nuxi": "^3.29.3",
    "nuxt-mcp": "^0.2.4",
    "prettier": "^3.6.2",
    "prisma": "^7.0.0",
    "sharp": "^0.34.4",
    "swagger-jsdoc": "^6.2.8",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.1"
  },
  "prisma": {
    "seed": "tsx ./prisma/seed.ts"
  },
  "packageManager": "yarn@1.22.22",
  "engines": {
    "node": ">=24.5.0",
    "yarn": "^1.22.22"
  }
}
