{
  "name": "admin",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./nuxt.config.ts",
  "description": "Nuxt 4 admin panel sa TailwindCSS i shared modulima",
  "objasnjenjenje": "Imports subpath za shared, koristi u kodu: import { helper } from '#shared/utils'",
  "objasnjenjenje_dva": "import { locales } from '#shared/utils/locale.ts'",
  "imports": {
    "#shared/*": "../../shared/*"
  },
  "author": "Aleksandar Stajic",
  "license": "MIT",
  "scripts": {
    "dev": "nuxt dev --port 3300",
    "build": "nuxt build",
    "preview": "nuxt preview",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "postinstall": "echo 'Admin postinstall complete'",
    "prepare:types": "pnpm nuxt prepare"
  },
  "dependencies": {
    "@iconify-json/lucide": "^1.2.69",
    "@iconify-json/openmoji": "^1.2.17",
    "@local/shared": "workspace:*",
