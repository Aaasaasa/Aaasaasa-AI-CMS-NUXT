    "@fontsource/inter": "^5.2.8",
    "@nuxt/schema": "^4.1.3",
    "@nuxtjs/color-mode": "^3.5.2",
    "@tailwindcss/postcss": "^4.1.14",
    "@tailwindcss/typography": "^0.5.19",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.21",
    "nuxt": "^4.1.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.14",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.4.0",
    "vite": "^7.1.9"
